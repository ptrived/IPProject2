import java.io.IOException;
import java.net.ServerSocket;


public class SimpleFTPServer {
	static ServerSocket socket ;
	static int portNum = 7735;
	
	
	public static void main(String args[]){
		try {
			socket = new ServerSocket(portNum);
			
		} catch (IOException e) {
			
			e.printStackTrace();
		}
	}
}
